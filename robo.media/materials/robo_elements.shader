textures/robo_elements/element_1
{
	qer_editorimage textures/robo_elements/element_1.tga
	surfaceparm alphashadow
	{
		map textures/robo_elements/element_1.tga
		rgbGen identity
		alphaFunc GE128
	}
	{
		map $lightmap 
		blendfunc filter
		rgbGen identity
		tcGen lightmap 
	}
}

